<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fsun.dao.report.mapper.SkuUseSoReportMapper">

	<parameterMap id="queryParamsMap" type="java.util.HashMap" >
	    <parameter property="shopId" mode="IN" jdbcType="VARCHAR"/>
	    <parameter property="startTime" mode="IN" jdbcType="VARCHAR"/>
	    <parameter property="endTime" mode="IN" jdbcType="VARCHAR"/>      
	</parameterMap>

	<select id="query" resultType="map" parameterType="com.fsun.domain.report.ReportBaseModel" >
		${select}
	</select>	
	
	<!-- 查询商品库存列表调用存储过程 -->	
	<select id="queryCall" statementType="CALLABLE" resultType="java.util.Map" parameterMap="queryParamsMap" > 
		exec sp_skuUseSoReport ?,?,?
	</select>
	
</mapper>